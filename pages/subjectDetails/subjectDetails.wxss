/**index.wxss**/
.clean:after{
  display: block;
  width: 0;
  height: 0;
  clear: both;
  content:"";
}
.rg{
  float: right;
  margin-right: 20rpx;
}
.lf{
  float: left;
}
.container{
  width: 90%;
  margin: 0 auto;
  font-size: 30rpx;
  height: 100%;
  padding-top: 30rpx;
}
.container .radius{
  float: left;
  width: 70%;
}
.container>view{
 width: 100%;
 text-align: center;
 margin-bottom: 30rpx;
}
.progress{
 margin-top: 7px; 
 width: 100%;
 border-radius: 10px;
}
.titlenumber{
  color: #fb7299
}

.title{
  text-align: left;
  font-size: 35rpx;
  margin-bottom: 30rpx;
}

.itemsbox{
  text-align: left;
  background: #f7f7f7;
  margin-bottom: 20rpx;
  padding: 20rpx 20rpx;
}
.itemsbox label{
  display: block;
  width: 100%;
  height: 100%;
}

.green{
  border: 1rpx solid green;
  color: green;
   border-radius: 30rpx;
}
.upimg{
  margin: 40rpx auto;
  text-align: center;
}
.usermotto button{
  width: 20%;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background-color: #fb7299;
  color: #fff;
  background: -webkit-gradient(linear, left top, left bottom, from(#fb7299), to(#E84B6E) );
}
