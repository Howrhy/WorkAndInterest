/**index.wxss**/
/* 公共样式表css */
.page {
  color: #333;
  margin: 0;
  height: 100%;
  font-weight: normal;
  font-family: ‘Trebuchet MS’, ‘Lucida Sans Unicode’, ‘Lucida Grande’, ‘Lucida Sans’, Arial, sans-serif;
}

/* * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
} */

a {
  text-decoration: none;
  color: #000;
}

img {
  border: 0;
}

body {
  background: #f8f8f8;
  color: #666;
}

td {
  margin: 0;
  padding: 0;
}

a {
  text-decoration: none;
  color: #08acee;
}

button {
  outline: 0;
}

img {
  border: 0;
}

button,
input,
optgroup,
select,
textarea {
  margin: 0;
  font: inherit;
  color: inherit;
  outline: none;
}

li {
  list-style: none;
}

a {
  color: #666;
}

a:hover {
  color: #eee;
}

.clearfix::after {
  justify-content: center;
  display: block;
  visibility: hidden;
  display: inline;
}

.clearfix {
  justify-content: center;
  align-items: center;
}

.clearfix-white {
  justify-content: center;
  align-items: center;
  background-color: #08acee;
  margin-bottom: 4rpx;
}


/* 必要布局样式css */
.ques-section {
  width: 690rpx;
  margin: 0 auto 20rpx;
  padding: 20rpx;
  border-radius: 4rpx;
  background-color: #fff;
}

.ques-section-item {
  width: fit-content;
  position: relative;
  float: left;
}

.ques-item-title {
  font-size: 36rpx;
  line-height: 48rpx;
  margin-bottom: 30rpx;
  font-weight: normal;
  color: #3335;
}

.ques-section-card {
  position: fixed;
  height: 100%;
  width: 100%;
  text-align: center;
  border-radius: 4rpx;
  background-color: #fafafa;
  align-items: center;
}

.ques-card-head {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 60rpx;
  padding-top: 96rpx;
  border-radius: 4rpx 4rpx 0 0;
}

.ques-card-gul {
  background-color: #f55727;
  background-image: -webkit-radial-gradient(50% 3%, circle, #f07a56, #f55727);
  background-image: radial-gradient(circle at 50% 3%, #f07a56, #f55727);
  height: 156rpx;
}

.ques-card-org {
  background-color: #f59b00;
  background-image: -webkit-radial-gradient(50% -6%, circle, #ffb83d 2%, #f59b00);
  background-image: radial-gradient(circle at 50% -6%, #ffb83d 2%, #f59b00);
  height: 156rpx;
}

.ques-card-bul {
  background-color: #A91219;
  background-image: -webkit-radial-gradient(50% -45%, circle, #A91219, #A91219);
  background-image: radial-gradient(circle at 50% -45%, #A91219, #A91219);
  height: auto;
}

.ques-card-title {
  font-size: 36rpx;
  font-weight: 500;
  line-height: 40rpx;
  position: relative;
  padding: 40rpx;
  color: #fff;
  justify-content: center;
}

.ques-card-text {
  line-height: 36rpx;
  position: relative;
  margin-top: 18rpx;
  opacity: .7;
  color: #fff;
  font-weight: normal;
}

.ques-card-num {
  overflow: hidden;
  margin: 80rpx auto 0;
  border-radius: 50%;
  position: relative;
  display: block;
  width: 160rpx;
  height: 160rpx;
}

.ques-card-num:before {
  position: absolute;
  top: 0;
  left: 0;
  width: 158rpx;
  height: 158rpx;
  content: '';
  border: 4rpx solid rgba(255, 255, 255, .9);
  border-radius: 100%;
}

.ques-card-num:hover:before {
  background-color: rgba(0, 0, 0, .3);
}

.ques-card-num img {
  width: 100%;
  height: auto;
  display: block;
  border: none;
}

.ques-card-name {
  font-size: 36rpx;
  line-height: 48rpx;
  margin-top: 20rpx;
}

.ques-card-major {
  line-height: 36rpx;
  margin-top: 12rpx;
  color: #a3a3a3;
}

.ques-card-field {
  font-size: 24rpx;
  line-height: 36rpx;
  height: 36rpx;
  margin-top: 50rpx;
  text-align: center;
  color: #000;
}

.ques-card-tag {
  font-size: 24rpx;
  margin: 12rpx 0 50rpx;
}

.ques-card-tag span {
  line-height: 34rpx;
  display: inline-block;
  max-width: 168rpx;
  height: 36rpx;
  padding: 0 12rpx;
  color: #fa9600;
  border: 2rpx solid #f5cc84;
  border-radius: 4rpx;
  margin: 0 8rpx;
}

.ques-card-help {
  display: block;
  margin: 40rpx;
  line-height: 40rpx;
  width: 180rpx;
  height: 60rpx;
  text-align: center;
  color: #fff;
  border-radius: 4rpx;
  background-color: #A91219;
}

.ques-card-bots {
  line-height: 36rpx;
  margin-top: 50rpx;
  color: #a3a3a3;
}

.ques-section-item+.ques-section-item {
  margin-left: 40rpx;
}

.ques-card-title-top {
  padding: 10rpx;
}

.ques-card-list {
  margin-top: 40rpx;
  text-align: left;
  height: 80rpx;
}

.ques-card-list li {
  position: relative;
}

.ques-card-list li:after {
  position: absolute;
  bottom: 0;
  left: -240rpx;
  display: block;
  width: 500rpx;
  height: 4rpx;
  content: '';
  background-color: #ededed;
}

.ques-list-box {
  position: relative;
  display: block;
  height: 60rpx;
  padding: 32rpx;
}

.ques-list-head {
  position: relative;
  float: left;
  padding-right: 16rpx;
  padding-top: 4rpx;
}

.ques-list-image {
  position: relative;
  border-radius: 50%;
  display: block;
  width: 72rpx;
  height: 72rpx;
  z-index: 2;
  margin: 0 auto;
}

.ques-list-image:before {
  position: absolute;
  top: 0;
  left: 0;
  width: 64rpx;
  height: 64rpx;
  content: '';
  border: 4rpx solid #f87851;
  border-radius: 100%;
}

.ques-list-image img {
  border-radius: 100%;
  width: 100%;
  height: auto;
  display: block;
  border: none;
}

.ques-list-name {
  overflow: hidden;
  margin-right: 66rpx;
}

.ques-list-name-head {
  padding-top: 10rpx;
  line-height: 32rpx;
  color: #333;
}

.ques-list-name-text {
  font-size: 24rpx;
  line-height: 32rpx;
  margin-top: 8rpx;
  color: #443f44;
}

.ques-list-name-icon {
  font-size: 24rpx;
  line-height: 48rpx;
  position: absolute;
  top: 50%;
  right: 28rpx;
  display: block;
  margin-top: -26rpx;
  text-align: center;
  color: #fff;
  width: 30rpx;
  height: 30rpx;
  background-image: url('data:image/png;base64,')
}

.item-icon001 {
  background-position: -62px -27px;
  width: 25px;
  height: 25px;
  display: block;
}

.item-icon002 {
  background-position: -62px 0;
  width: 25px;
  height: 25px;
}

.item-icon003 {
  background-position: -28px -32px;
  width: 25px;
  height: 25px;
}

.item-icon000 {
  background-position: 0 -60px;
  width: 25px;
  height: 25px;
}

.ques-list-box:hover {
  cursor: pointer;
  background-color: #f5f5f5;
}