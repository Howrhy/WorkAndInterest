/**index.wxss**/
.clean:after{
  display: block;
  width: 0;
  height: 0;
  clear: both;
  content:"";
}
.container{
  width: 90%;
  margin: 0 auto;
  font-size: 30rpx;
  height: 100%;
  padding-top: 30rpx;
}
.container .radius{
  float: left;
  width: 70%;
}
.container>view{
 width: 100%;
 text-align: center;
 margin-bottom: 30rpx;
}
.progress{
 margin-top: 7px; 
 width: 100%;
 border-radius: 10px;
}
.titlenumber{
  color: #fb7299
}

.title{
  text-align: left;
  font-size: 35rpx;
  margin-bottom: 30rpx;
}

.upimg{
  margin: 0rpx auto 40rpx auto;
  text-align: center;
}

.itemsbox{
  text-align: left;
  background: #f7f7f7;
  margin-bottom: 20rpx;
  padding: 20rpx 20rpx;
}
.itemsbox label{
  display: block;
  width: 100%;
  height: 100%;
}

.usermotto button{
  margin-bottom: 20rpx;
  background-color: #fb7299;
  color: #fff;
  background: -webkit-gradient(linear, left top, left bottom, from(#fb7299), to(#E84B6E) );
}

.tips{
  margin: 100rpx auto;
  color: #e6e6e6;
  text-align: center;
}

.but{
   background-color: #fb7299;
   color: #fff;
   margin-top: 60%;
   width: 20%;
   background: -webkit-gradient(linear, left top, left bottom, from(#fb7299), to(#E84B6E) );
}